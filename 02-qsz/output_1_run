def1ch, NRDOTS=1
H0=coefzeta[1, 0]*(-1 + nc[f[0, 0, 0], f[1, 0, 0]] + nc[f[0, 0, 1], f[1, 0, 1]])
"self[CR,UP]="-0.15*d[0, 1] - nc[d[0, 1], B*(-nc[d[0, 0], d[1, 0]] + nc[d[0, 1], d[1, 1]])]/2 + nc[B*(-nc[d[0, 0], d[1, 0]] + nc[d[0, 1], d[1, 1]]), d[0, 1]]/2 - 0.5*nc[d[0, 0], d[0, 1], d[1, 0]]
"SigmaH[UP]="-0.15 + 0.5*nc[d[0, 0], d[1, 0]] + (B*nc[d[0, 0], d[1, 0]])/2 - (B*nc[d[0, 1], d[1, 1]])/2 + nc[d[0, 1], d[1, 1], B*(-nc[d[0, 0], d[1, 0]] + nc[d[0, 1], d[1, 1]])]/2 - 
   nc[d[0, 1], B*(-nc[d[0, 0], d[1, 0]] + nc[d[0, 1], d[1, 1]]), d[1, 1]]/2 + nc[d[1, 1], B*(-nc[d[0, 0], d[1, 0]] + nc[d[0, 1], d[1, 1]]), d[0, 1]]/2 + nc[B*(-nc[d[0, 0], d[1, 0]] + nc[d[0, 1], d[1, 1]]), d[0, 1], d[1, 1]]/2
