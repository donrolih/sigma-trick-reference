"SigmaH[DO]="-0.15 + (B*nc[d[0, 0], d[1, 0]])/2 + 0.5*nc[d[0, 1], d[1, 1]] - (B*nc[d[0, 1], d[1, 1]])/2 + nc[d[0, 0], d[1, 0], B*(-nc[d[0, 0], d[1, 0]] + nc[d[0, 1], d[1, 1]])]/2 - 
   nc[d[0, 0], B*(-nc[d[0, 0], d[1, 0]] + nc[d[0, 1], d[1, 1]]), d[1, 0]]/2 + nc[d[1, 0], B*(-nc[d[0, 0], d[1, 0]] + nc[d[0, 1], d[1, 1]]), d[0, 0]]/2 + nc[B*(-nc[d[0, 0], d[1, 0]] + nc[d[0, 1], d[1, 1]]), d[0, 0], d[1, 0]]/2
