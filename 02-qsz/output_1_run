"self[CR,UP]="-0.15*d[0, 1] - nc[d[0, 1], B*(-nc[d[0, 0], d[1, 0]] + nc[d[0, 1], d[1, 1]])]/2 + nc[B*(-nc[d[0, 0], d[1, 0]] + nc[d[0, 1], d[1, 1]]), d[0, 1]]/2 - 0.5*nc[d[0, 0], d[0, 1], d[1, 0]]
